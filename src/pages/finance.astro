<BaseLayout>

  <!-- NAVIGATION BAR -->

  <nav style="background-color: #fff; box-shadow: 0 1px 0 0 rgba(0, 0, 0, 0.1);">
    <div style="max-width: 1240px; margin: 0 auto; padding: 1rem; display: flex; justify-content: space-between; align-items: center;">
      <div style="display: flex; align-items: center; gap: 0.5rem;">
        <img src={navLinks[0].logo} alt="Logo" style="height: 32px; width: auto;" />
        <a href={navLinks[0].link} 
          class="inline-flex items-center justify-center font-medium"
          style="text-decoration: none; color: #5046e4; font-weight: 500; transition: color 0.2s ease;"
          onmouseover="this.style.color='#333';"
          onmouseout="this.style.color='#5046e4';">
          {navLinks[0].title}
        </a>
      </div>
      <ul style="list-style: none; display: flex; gap: 2rem; margin: 0; padding: 0;">
        {navLinks.slice(1).map(link => (
          <li key={link.href}>
            <a href={link.href} 
              class="inline-flex items-center justify-center font-medium"
              style="text-decoration: none; color: #5046e4; font-weight: 500; transition: color 0.2s ease;"
              onmouseover="this.style.color='#333';"
              onmouseout="this.style.color='#5046e4';">
              {link.name}
            </a>
          </li>
        ))}
      </ul>
    </div>
  </nav>
  


  <section>
    <div
      class="mx-auto max-w-7xl lg:px-16 md:px-12 px-8 xl:px-24 mt-24 z-50 items-center">
