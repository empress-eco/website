        <div>
          <div class="lg:pr-24 md:pr-12 pb-12 lg:pb-0">
            <h2
              class="text-purple-900 font-semibold mt-6 text-4xl lg:text-7xl tracking-tighter">
              {sectionOne.title}
            </h2>
            <p class="text-slate-500 mt-5">
              {sectionOne.description}
            </p>
          </div>
        </div>
      </div>
      <a href="/suite/apps" 
      class="inline-flex items-center justify-center px-4 py-2 rounded-full font-medium mr-4 mt-5"
      style="background-color: #5046e4; color: #fff; border: px solid #6767ff; border-radius: 30px; box-shadow: 2px 2px 5px rgba(0, 0, 0, 0.2), -2px -2px 5px rgba(255, 255, 255, 0.7); transition: all 0.2s ease; font-size: 0.875rem; height: 2.625rem;"
      onmouseover="this.style.backgroundColor='#6767ff'; this.style.borderColor='#5046e4';"
      onmouseout="this.style.backgroundColor='#5046e4'; this.style.borderColor='#6767ff';"
      onmousedown="this.style.backgroundColor='#3838b1'; this.style.borderColor='#6767ff';"
      onmouseup="this.style.backgroundColor='#5046e4'; this.style.borderColor='#6767ff';">
      {sectionOne.buttonText}
   </a>
   
   <a href="/suite/apps" 
   class="inline-flex items-center justify-center px-4 py-2 rounded-full font-medium"
   style="background-color: #fff; color: #333; border: none; border-radius: 30px; box-shadow: 4px 4px 8px rgba(0, 0, 0, 0.1), -4px -4px 8px rgba(255, 255, 255, 0.9); transition: all 0.2s ease; font-size: 0.875rem; height: 2.625rem;"
   onmouseover="this.style.boxShadow='inset 4px 4px 8px rgba(0, 0, 0, 0.1), inset -4px -4px 8px rgba(255, 255, 255, 0.9)';"
   onmouseout="this.style.boxShadow='4px 4px 8px rgba(0, 0, 0, 0.1), -4px -4px 8px rgba(255, 255, 255, 0.9)';"
   onmousedown="this.style.boxShadow='inset 4px 4px 8px rgba(0, 0, 0, 0.1), inset -4px -4px 8px rgba(255, 255, 255, 0.9)';"
   onmouseup="this.style.boxShadow='4px 4px 8px rgba(0, 0, 0, 0.1), -4px -4px 8px rgba(255, 255, 255, 0.9)';">
   {sectionOne.buttonTextTwo}
</a>
    </div>
  
    <section>
