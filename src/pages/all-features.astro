---
import BaseLayout from "../layouts/BaseLayout.astro";
import NavigationOne from "../components/navigation/NavigationOne.astro";
import FeatureOne from "../components/features/FeatureOne.astro";
import FeatureTwo from "../components/features/FeatureTwo.astro";
import FeatureThree from "../components/features/FeatureThree.astro";
import FeatureFour from "../components/features/FeatureFour.astro";
import FeatureFive from "../components/features/FeatureFive.astro";
import FeatureSix from "../components/features/FeatureSix.astro";
import FeatureSeven from "../components/features/FeatureSeven.astro";
import FeatureEight from "../components/features/FeatureEight.astro";
import FeatureNine from "../components/features/FeatureNine.astro";
import FeatureTen from "../components/features/FeatureTen.astro";
import FeatureEleven from "../components/features/FeatureEleven.astro";
import FeatureTwelve from "../components/features/FeatureTwelve.astro";
import FooterTwo from "../components/footers/FooterTwo.astro";
import { AstroSeo } from "@astrolib/seo";
---

<!---
Use AstroSeo in all the pages you want different Seo than the index page
-->
<AstroSeo
  title="All features"
  description="Your site description goes here"
  canonical="https://www.supremeui.dev"
  openGraph={{
    url: "https://www.supremeui.dev",
    title: "Open Graph Title",
    description: "Open Graph Description",
    images: [
      {
        url: "https://www.supremeui.dev/opengraph/social-og.jpg",
        width: 1200,
        height: 630,
        alt: "Social open graph",
        type: "image/jpeg",
      },
      {
        url: "https://www.supremeui.dev/opengraph/twitter-og.jpg",
        width: 4096,
        height: 4096,
        alt: "Twitter open graph",
        type: "image/jpeg",
      },
      { url: "https://www.supremeui.dev/opengraph/social-og.jpg" },
      { url: "https://www.supremeui.dev/opengraph/twitter-og.jpg" },
    ],
    site_name: "YourSitesName",
  }}
  twitter={{
    handle: "@TwitterHandle",
    site: "@site",
    cardType: "summary_large_image",
  }}
/>
<BaseLayout>
  <NavigationOne />
  <FeatureOne />
  <FeatureTwo />
  <FeatureThree />
  <FeatureFour />
  <FeatureFive />
  <FeatureSix />
  <FeatureSeven />
  <FeatureEight />
  <FeatureNine />
  <FeatureTen />
  <FeatureEleven />
  <FeatureTwelve />
  <FooterTwo />
</BaseLayout>
